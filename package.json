{
  "name": "@yunfeidog/openapi2typescript",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.3",
  "description": "根据openapi3.0规范生成typescript代码",
  "repository": {
    "type": "git",
    "url": "git@github.com:CodeGenieGenerator/openapi2typescript.git"
  },
  "license": "MIT",
  "author": "蔡徐坤",
  "main": "dist/index.js",
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build && np --no-cleanup --yolo --no-publish --any-branch",
    "start": "tsc -w",
    "test": "rm -rf ./test/gen/  && ts-node ./test/test.ts && cd ..",
    "casll-macos": "rm -rf ./test/casll/ && ts-node ./test/casll.ts && cd ..",
    "casll-windows": "rimraf ./test/casll/ && ts-node ./test/casll.ts && cd .."
  },
  "dependencies": {
    "@umijs/fabric": "^2.5.6",
    "chalk": "^4.1.2",
    "dayjs": "^1.10.3",
    "glob": "^7.1.6",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "node-fetch": "^2.6.1",
    "nunjucks": "^3.2.2",
    "openapi3-ts": "^2.0.1",
    "prettier": "^2.2.1",
    "reserved-words": "^0.1.2",
    "rimraf": "^3.0.2",
    "swagger2openapi": "^7.0.4",
    "tiny-pinyin": "^1.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "np": "^7.2.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.0",
    "typescript": "^4.1.3"
  }
}
